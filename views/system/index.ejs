<% layout('../layouts/default.ejs') -%>

<div class="row">
    <div class="col-md-12">
        <div class="page-header">
            <h3>System</h3>
        </div>

        <div class="col-md-8">
        <form role="form" action="/system/update" method="post">
            <fieldset>
                <div class="form-group">
                    <label for="vagrant-config-vm-box">Project Name</label>
                    <input type="text" class="form-control" id="config-name" value="<%=config.name%>" name="config_name">
                </div>

                <button type="submit" class="btn btn-primary">Save</button>
                <a href="/">Cancel</a>
            </fieldset>
        </form>
        </div>
    </div>
</div>